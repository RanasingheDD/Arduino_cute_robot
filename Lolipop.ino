void eyeblink(){
  for(int16_t i=0; i<30; i+=5) {
    display.fillRect(0, 0, 132, 10+i, BLACK);  
    display.display();
    delay(1);
    }
  }

void setup() {
  Serial.begin(9600);
   if(!display.begin(SSD1306_SWITCHCAPVCC, 0x3C)) {
    Serial.println(F("SSD1306 allocation failed"));
    for(;;);
  }

  display.clearDisplay();

  pinMode(touchPin, INPUT);
  display.setTextSize(2);
  display.setTextColor(SSD1306_WHITE);
  display.setCursor(0,0);
  display.println(F("Hello"));
  display.println();
  display.setTextSize(2);
  display.println(F("I'am Lolipop"));
  display.display();
  delay(2000);

  display.clearDisplay(); 
 
  randomSeed(analogRead(0));

  blinkInterval = random(2000, 10000); 

}

void loop() {

  int touchState = digitalRead(touchPin);
  unsigned long currentTime = millis();

  if (touchState == HIGH) {
    Serial.println("Touched");

    if (count == 0) {
      firstTouchTime = currentTime;
    }

    count++;
    delay(100);
  } else {
    Serial.println("Not Touched");
  }

  if (count >= 2 && (currentTime - firstTouchTime) <= timeWindow) {
    display.drawBitmap(0, 0, image_data_EYES_ANGRYarray, 128, 64, 1);
    display.display();
    count = 0;
    delay(2000);
    display.drawBitmap(0, 0, image_data_DISTRESSED_EYESarray, 128, 64, 1);
    display.display();
    delay(1000);
    display.clearDisplay();

    count = 0;
    firstTouchTime = 0;
  } else if ((currentTime - firstTouchTime) > timeWindow) {
    count = 0;
    firstTouchTime = 0;
  }
  delay(100);
  display.clearDisplay();  

  unsigned long currentMillis = millis();

  if (currentMillis - previousMillis >= blinkInterval) {
    previousMillis = currentMillis;
    
    eyeblink();
    
    blinkInterval = random(2000, 10000); 
  }
  display.drawBitmap(0, 0, image_data_EYES_FRONTarray, 128, 64, 1);
  display.display();


  display.display();
  delay(1000); 
  display.clearDisplay();  

  Serial.println(count);

}